programa {
  funcao inicio() {
    
    // Deckaração de variáveis
    inteiro numeroUm , numeroDois, soma

    // Entrada de dados 
    escreva ("Digite o primeiro número: ")
    leia (numeroUm)

    escreva ("Digite o segundo número: ")
    leia (numeroDois)

    // Processamento]
    soma = numeroUm + numeroDois

    // Saída de dados
    escreva ("A soma = ", soma)
    
  }
}
